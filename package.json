{
  "name": "tsd",
  "version": "0.6.0-beta.4",
  "description": "A package manager for TypeScript definitions",
  "main": "dist/tsd.js",
  "bin": {
    "tsd": "dist/bin/tsd.js"
  },
  "preferGlobal": true,
  "files": [
    "dist/",
    "tsd.json",
    "LICENSE"
  ],
  "scripts": {
    "lint": "# TODO",
    "build": "rm -rf dist && npm run build-ts",
    "build-ts": "tsc",
    "test-spec": "mocha \"dist/**/*.spec.js\" -R spec --bail --require test/support/common.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- \"dist/**/*.spec.js\" -R spec --bail --require test/support/common.js",
    "test": "npm run build && npm run lint && npm run test-cov",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DefinitelyTyped/tsd.git"
  },
  "typescript": {
    "definition": "tsd.d.ts"
  },
  "keywords": [
    "typescript",
    "definition",
    "package",
    "manager"
  ],
  "author": "Blake Embrey <hello@blakeembrey.com>",
  "contributors": [
    "Diullei Gomes (https://github.com/Diullei)",
    "Bart van der Schoor (https://github.com/Bartvds)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DefinitelyTyped/tsd/issues"
  },
  "homepage": "https://github.com/DefinitelyTyped/tsd",
  "devDependencies": {
    "chai": "^2.3.0",
    "chai-as-promised": "^5.1.0",
    "istanbul": "^0.3.13",
    "mocha": "^2.1.0",
    "pre-commit": "^1.0.6",
    "typescript": "^1.5.0-alpha"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "es6-promise": "^2.1.1",
    "fs-promise": "^0.3.1",
    "graceful-fs": "^3.0.6",
    "invariant": "^2.0.0",
    "mkdirp": "^0.5.1",
    "popsicle": "^0.5.12",
    "proxyquire": "^1.4.0",
    "read-installed": "^4.0.0",
    "semver": "^4.3.4",
    "sort-keys": "^1.0.0",
    "thenify": "^3.1.0",
    "xtend": "^4.0.0"
  }
}
